{{ 'footer.css' | asset_url | stylesheet_tag }}

<div class="footer-wrapper">
    <div class="footer">
        <div class="footer-links">
            {% for link in linklists.main-menu.links %}
                <a href="{{ link.url }}">{{ link.title }}</a>
            {% endfor %}
        </div>
         
        <div>
            <img class="footer-logo" src="{{ 'cat.PNG' | asset_url }}" alt="logo" />
        </div> 

    </div>
</div> 